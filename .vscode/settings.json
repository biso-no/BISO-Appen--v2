{
    "i18n-ally.localesPaths": [
        "i18n/locales"
    ],
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.displayLanguage": "en",
    "i18n-ally.keystyle": "nested",
    "i18n-ally.namespace": false,
    "i18n-ally.pathMatcher": "{locale}/translation.json",
    "i18n-ally.enabledFrameworks": ["i18next", "react", "react-i18next"],
    "i18n-ally.sortKeys": true,
    "i18n-ally.keepFulfilled": true,
    "i18n-ally.extract.keygenStrategy": "slug",
    "i18n-ally.extract.keygenStyle": "camel",
    "i18n-ally.extract.autoDetect": true,
    "i18n-ally.extract.targetPickingStrategy": "most-similar",
    "i18n-ally.extract.parsers.html": {
        "attributes": ["label", "placeholder", "title", "description", "accessibilityLabel", "testID"],
        "ignoredTags": ["script", "style"],
        "vBind": true,
        "inlineText": true
    },
    "i18n-ally.extract.parsers.babel": {
        "attributes": ["label", "placeholder", "title", "description", "accessibilityLabel", "testID"]
    },
    "i18n-ally.regex.usageMatch": [
        "[^\\w\\d]t\\(['\"`]({key})['\"`]",
        "[^\\w\\d]i18next\\.t\\(['\"`]({key})['\"`]"
    ],
    "i18n-ally.refactor.templates": [
        {
            "source": "jsx-text",
            "templates": ["{t('{key}')}"]
        },
        {
            "source": "html-attribute",
            "templates": ["{t('{key}')}"]
        },
        {
            "source": "html-inline",
            "templates": ["{t('{key}')}"]
        },
        {
            "source": "js-string",
            "templates": ["t('{key}')", "i18next.t('{key}')"]
        },
        {
            "source": "js-template",
            "templates": ["${t('{key}')}", "${i18next.t('{key}')}"]
        }
    ],
    "i18n-ally.translate.engines": ["google"],
    "i18n-ally.translate.parallels": 15,
    "i18n-ally.translate.saveAsCandidates": true,
    "i18n-ally.editor.preferEditor": true,
    "i18n-ally.indent": 2,
    "i18n-ally.tabStyle": "space",
    "i18n-ally.review.enabled": true,
    "i18n-ally.regex.key": "[\\.\\w\\d\\-_]*?",
    "i18n-ally.theme.annotation": "rgba(153, 153, 153, .8)",
    "i18n-ally.theme.annotationMissing": "#f44336"
}